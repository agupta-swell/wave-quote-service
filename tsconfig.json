{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "target": "es2017",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "incremental": true
    // "noImplicitAny": false
  },
  "exclude": ["node_modules", "dist"]
}
